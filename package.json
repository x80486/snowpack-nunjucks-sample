    "snowpack": "3.8.8",
    "snowpack-plugin-nunjucks": "0.0.3"
  },
  "dependencies": {
    "jquery": "3.6.0"
  },
  "engines": {
    "node": "16.x"
  },
  "prettier": {
    "bracketSameLine": true,
    "endOfLine": "lf",
    "printWidth": 120,
    "quoteProps": "consistent",
    "tabWidth": 2
  }
